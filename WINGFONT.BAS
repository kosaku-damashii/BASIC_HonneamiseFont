LABEL PSTR
VAR A$,L,C
A$=ARGS$(1)
L=LEN(A$)
FOR I=0 TO L-1
  C=ASC(A$(I,1))
  IF C >= $B6 THEN
    IF C <= $C4 THEN
      IF ASC(A$(I+1,1)) = ASC("Þ") THEN
        C=C+42
        I=I+1
      ENDIF
    ENDIF
  ENDIF
  IF C >= $CA THEN
    IF C <= $CE THEN
      IF ASC(A$(I+1,1)) = ASC("Þ") THEN
        C=C+37
        I=I+1
      ELSEIF ASC(A$(I+1,1)) = ASC("ß") THEN
        C=C+37+5
        I=I+1
      ENDIF
    ENDIF
  ENDIF
  PRINT CHR$(C);
NEXT
PRINT
RETURN

